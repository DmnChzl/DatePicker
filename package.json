    "dist",
    "README.md"
  ],
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "author": {
    "name": "Damien Chazoule",
    "email": "dmnchzl@pm.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmnchzl/datepicker"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write"
  },
  "dependencies": {
    "polished": "~4.2.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "styled-components": "~5.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~13.4.0",
    "@types/react": "~18.0.17",
    "@types/react-dom": "~18.0.6",
    "@types/styled-components": "~5.1.25",
    "@vitejs/plugin-react": "~2.1.0",
    "husky": "~8.0.0",
    "jsdom": "~20.0.0",
    "lint-staged": "~13.0.0",
    "prettier": "~2.7.0",
    "typescript": "~4.8.4",
    "vite": "~3.1.5",
    "vitest": "~0.23.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
